
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Comida {
  id            Int @id @default(autoincrement())
  nome          String
  tipo          String
  preco         Float
  descricao     String
  createdAt     DateTime @default(now())
  updateAt      DateTime @updatedAt

  @@map("Comidas")
}